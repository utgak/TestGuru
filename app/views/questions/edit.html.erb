<h1>Edit <%= question_header %> Question</h1>
<%= form_with model: [@question], local: true do |form|%>
  <%= render 'shared/errors', resource: @question %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  <p><%= form.submit %></p>


<% end %>
<%= link_to 'Back', test_questions_url(@question.test_id) %>