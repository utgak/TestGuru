<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'shared/errors', resource: @test %>

  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title, id: :test_title %>
  </p>

  <p>
    <%= form.label :level %><br>
    <%= form.text_field :level, id: :test_level %>
  </p>

  <p>
    <%= form.label :time_limit_in_minutes %><br>
    <%= form.text_field :time_limit_in_minutes, id: :test_time_limit_in_minutes %>
  </p>

  <p>
    <%= form.label :category_id %><br>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p><%= form.submit %></p>
<% end %>