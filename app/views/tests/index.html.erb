<h1>Tests</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Number of questions</th>
    <th>Action</th>
  </tr>

  <% Test.all.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.questions.size %></td>
      <td><%= link_to 'Show', test_path(test) %></td>
    </tr>
  <% end %>
</table>